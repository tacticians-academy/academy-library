		"ts-node": "^10.9.1",
		"typescript": "^5.3.3"
	},
	"eslintConfig": {
		"extends": "@ky-is/eslint-config/typescript",
		"rules": {
			"@typescript-eslint/strict-boolean-expressions": [
				"warn",
				{
					"allowNullableEnum": true
				}
			]
		}
